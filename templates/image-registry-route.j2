apiVersion: route.openshift.io/v1
kind: Route
metadata:
  name: image-registry
  namespace: openshift-image-registry
  resourceVersion: "222467"
spec:
  host: image-registry.{{ openshift_ingress_default_subdomain }}
  port:
    targetPort: 5000-tcp
  subdomain: ""
  to:
    kind: Service
    name: image-registry
    weight: 100
  wildcardPolicy: None
